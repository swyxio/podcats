    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/xml": "^1.0.2",
    "auto-changelog": "^1.13.0",
    "gh-release": "^3.5.0",
    "husky": "^2.3.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "tsdx": "^0.5.11",
    "typescript": "^3.2.4",
    "xml": "^1.0.1"
  }
}
