{
  "name": "podcats",
  "version": "0.1.11",
  "main": "dist/index.js",
  "umd:main": "dist/podcats.umd.production.js",
  "module": "dist/podcats.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
